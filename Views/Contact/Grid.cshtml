@model IEnumerable<CONTACT_US>

<div class="table-responsive">
    <table id="list" class="table table-bordered table-striped table-hover table-condensed">
        <thead>
            <tr>
                <th class="hidden">KEY</th>
                <th>姓名</th>
                <th>電子信箱</th>
                <th>訊息內容</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var it in Model)
            {
            <tr class="alert-info">
                <td class="hidden">@Html.DisplayFor(m => it.GUID)</td>
                <td>@Html.DisplayFor(m => it.NAME)</td>
                <td>@Html.DisplayFor(m => it.EMAIL)</td>
                <td>@Html.DisplayFor(m => it.MESSAGE)</td>
            </tr>
            }
        </tbody>
    </table>
</div>
